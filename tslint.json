{
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "quotemark": [
      true,
      "single"
    ],
    "no-console": [
      false
    ],
    "object-literal-sort-keys": false,
    "space-before-function-paren": false,
    "no-unused-expression": [
      true,
      "allow-fast-null-checks"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }
    ]
  }
}
